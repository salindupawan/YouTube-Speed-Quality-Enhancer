{
  "manifest_version": 3,
  "name": "YouTube Speed & Quality Enhancer",
  "version": "1.0",
  "description": "Custom controls for YouTube playback.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "YouTube Enhancer"
  },
  "content_scripts": [
  {
    "matches": ["*://www.youtube.com/*"],
    "js": ["content.js"],
    "run_at": "document_end"
  }
]
}